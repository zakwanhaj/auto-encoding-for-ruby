{
  "name": "Auto Encoding for Ruby",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Atom package to automatically include #encoding: utf-8 on Ruby files",
  "activationCommands": {
    "atom-workspace": "auto-encoding-for-ruby:add-encoding",
    ".tree-view .file .name": "auto-encoding-for-ruby:add-encoding-file",
    ".tree-view .directory .name": "auto-encoding-for-ruby:add-encoding-directory"
},
  "repository": "https://github.com/zakwanhaj/auto-encoding-for-ruby.git",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
  }
}
